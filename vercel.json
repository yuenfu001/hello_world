{
    "version": 1,
    "builds": [
    {
        "src": "form/wsgi.py",
        "use": "@vercel/python",
        "config": {
        "maxLambdaSize": "15mb",
        "runtime":"python3.9"
        }
    }

    ],
    "routes": [
   
    {
        "src": "/(.*)",
        "dest": "from/wsgi.py"
    }
    ]
    }